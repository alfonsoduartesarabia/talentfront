{
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/jsdom": "^16.2.1",
    "@types/morgan": "^1.7.37",
    "@types/node": "^10.9.4",
    "@types/query-string": "6.1.0",
    "@types/ramda": "^0.27.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "typescript": "^4.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "homepage": "/about/",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md}": [
      "prettier --write"
    ]
  },
  "name": "about-app",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" && yarn lint:fix",
    "lint": "eslint --ext .tsx,.ts,.js,.jsx src/",
    "lint:fix": "eslint --ext .tsx,.ts,.js,.jsx src/ --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "version": "0.1.0"
}
